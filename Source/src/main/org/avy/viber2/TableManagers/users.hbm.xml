<?xml version="1.1" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD //EN" "https://hibernate.org/dtd/hibernate-mapping.dtd">
<hibernate-mapping>
	<class name="org.avy.viber2.Database.Users" table="users">
		<id name="id" type="int" column="id">
			<!-- правилен клас? -->
			<generator class="increment" />
		</id>
		<property name="uname" column="name" type="string" not-null="true" length="50"></property>
		<property name="passw" column="password" type="string" not-null="true" length="97"></property>
	</class>
</hibernate-mapping>
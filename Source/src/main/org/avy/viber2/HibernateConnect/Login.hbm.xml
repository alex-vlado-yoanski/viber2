<?xml version="1.1" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD //EN" "https://hibernate.org/dtd/hibernate-mapping.dtd">
<hibernate-mapping>
	<class name="org.avy.viber2.Login" table="users">
		<id name="id">
			<generator class="assigned"></generator>
		</id>
		<property name="name"></property>
		<property name="password"></property>
	</class>
</hibernate-mapping>
